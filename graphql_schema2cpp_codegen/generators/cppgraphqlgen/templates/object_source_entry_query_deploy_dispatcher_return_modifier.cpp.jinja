{% from '_macros.cpp.jinja' import make_cpp_type %}
{% macro object_source_entry_query_deploy_dispatcher_return_modifier(field, deploy) %}
    {%- set value -%}
        m_Modifiers->get{{ field.name | upper_first_letter }}
            {%- if deploy.attribute -%}From{{ deploy.attribute }}{%- endif -%}()
    {%- endset %}
    return {{ make_cpp_type(field.type, value, enable_optional=True) }};
{%- endmacro %}
